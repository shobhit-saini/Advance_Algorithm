#include<iostream>
#include<map>
using namespace std;
int main()
{
	map<int,int> mp;
	int n,value=0,max=0;
	cout<<"Enter the size of an array:";
	cin>>n;
	int* ar=new int[n];
	cout<<"Enter the elements:\n";
	for(int i=0; i<n; i++)
	{
		cin>>ar[i];
	}
	for(int i=0; i<n; i++)
	{
		mp[ar[i]]+=1;
	}
 	map<int, int>::iterator it;

        for(it = mp.begin(); it != mp.end(); it++)
        {
		
            if(max<=it->second)
		{
			max=it->second;
			value=it->first;
		}
	}
	if(max>n/2)
	{
		cout<<value;
	}
	else
	{
		cout<<"0";
	}

	
}
